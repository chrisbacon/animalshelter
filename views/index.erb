<h1>Pets</h1>

<div id="content">
    <% for pet in @pets %>
        <ul class="pet">
            <li>name: <%= pet.name %></li>
            <li>species: <%= pet.species %></li>
            <li>entry: <%= pet.entry %></li>
            <% if pet.owner %>
            <li>owner: <%= pet.owner.name %></li>
            <% else %>
            <li>available: <%= pet.is_adoptable? %></li>
            <% end %>
            

        </ul>
    <% end %>
</div>